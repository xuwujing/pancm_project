<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zans.portal.dao.SysCustomDataMapper" >

    <select id="findCountByFieldId" resultType="java.lang.Integer">
        select count(1) from sys_custom_data s where s.field_id = #{fieldId}
    </select>

</mapper>